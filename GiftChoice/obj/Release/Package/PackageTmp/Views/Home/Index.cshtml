@{
    ViewBag.Title = "Home Page";
}


<!-- Product Slider CDN -->
<link rel='stylesheet' href='https://unpkg.com/swiper/swiper-bundle.min.css'>


<!-- Main Content Start GiftChoice V4  -->
<main class="main">

    <!-- Slider Start -->
    <section class="p-0 home-slider" ng-init="SliderList()">
        <div id="carouselId" class="carousel slide" data-bs-ride="carousel">
            <div class="carousel-inner" role="listbox">
                <div ng-repeat="s in SliderListData" class="carousel-item {{$index==0?'active':''}} ">
                    <a href="{{s.SUrl}}">
                        <img src="~/images/SliderImg/{{s.SliderImage}}" class="w-100 d-block img-fluid" loading="lazy" alt="corporates-gifts" />
                    </a>
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselId" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselId" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
    </section>
    <!-- Slider End -->
    @* Category New Slider Start *@
    <div class="container">
        <section id="cate-product" class="caterody-product-slider">

            <!-- Title With Arrows -->
            <div class="row align-items-center pb-3">
                <div class="col">
                    <h5 class="section-title mb-0"><span>Shop By</span> Categories</h5>
                </div>
                <div class="col clearfix">
                    <span class="title-section d-flex justify-content-end align-items-center">
                        <button class="btn btn-archive btn-sm prev-btn arrow-button" type="button">
                            <i class="fa fa-angle-left"></i>
                            <span class="visually-hidden">Previous</span>
                        </button>

                        <span class="mx-1">&nbsp;</span>

                        <button class="btn btn-archive btn-sm next-btn arrow-button" type="button">
                            <i class="fa fa-angle-right"></i>
                            <span class="visually-hidden">Next</span>
                        </button>
                    </span>
                </div>
            </div>

            <!-- Products Slider Code -->
            <div class="swiper-container" ng-init="GetHomeMainCateHeroData()" ng-show="HomeHeroMainCateData.length>0">
                <div class="swiper-wrapper">
                    <div class="swiper-slide" ng-repeat="m in HomeHeroMainCateData">
                        <div class="col">
                            <div class="card">
                                <a href="/Home/shop?Main={{m.MTitle}}">
                                    <img ng-src="~/images/MainCate/{{m.MImage}}" alt="{{m.MUrl}}" loading="lazy">
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
