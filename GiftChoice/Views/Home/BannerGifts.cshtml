
@{
    /**/

    /**/

    ViewBag.Title = "BannerGifts";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Product Slider CDN -->
<link rel='stylesheet' href='https://unpkg.com/swiper/swiper-bundle.min.css'>


<main class="main" ng-init="GetBannerAllProductwithmodel()">

    <!-- Page Breadcrumb Start -->
    <div class="page-header breadcrumb-wrap">
        <div class="container">
            <div class="breadcrumb">
                <a href="/Home/Index" rel="nofollow">Home</a>
                <span></span> Banner Category
            </div>
        </div>
    </div>
    <!-- Page Breadcrumb End -->


    <section class="mt-50 mb-50">
        <div class="container">
            <div class="row flex-row-reverse gx-1 g-1">
                <div class="col-lg-12">
                    <section class="product-tabs section-padding mb-0 pb-1"  ng-repeat="f in FilterKeywordList" ng-show="FilterKeywordList.length>0">
                        <div class="container">
                            <div class="row g-2">
                                <div class="col-lg-12 col-sm-12 col-12">
                                    <div class="bg-light  background-banner">
                                        <div class="content-data p-3 pp-3">
                                            <div id="friends-product" class="friends-product-slider" style="overflow : hidden !important;">

                                                <!-- Title With Arrows -->
                                                <div class="row align-items-center pb-3">
                                                    <div class="col">
                                                        <h5 class="section-title mb-0">{{f.Filterkeyword}}</h5>
                                                    </div>
                                                    <div class="col clearfix">
                                                        <span class="title-section d-flex justify-content-end align-items-center">
                                                            <a href="/Home/giftfilterkeyword?ProductType=giftsFriendsbestsellers" type="button" class="btn btn-theme-1">View All</a>
                                                        </span>
                                                    </div>
                                                </div>

                                                <!-- Products Slider Code -->
                                                <div class="friends-product">
                                                    <div class="swiper-wrapper">
                                                        <div class="swiper-slide" ng-repeat="q in f.ProductList">
                                                            <div class="col">
                                                                <div class="product-cart-wrap mb-10 ">
                                                                    <div class="product-img-action-wrap">
                                                                        <div class="product-img product-img-zoom">
                                                                            <a href="/Home/Gift?url={{q.PUrl}}">
                                                                                <img class="default-img img-fluid" src="~/images/ProductImg/{{q.ProductImage}}" loading="lazy" alt="product-image" loading="lazy" alt="">
                                                                                <img class="hover-img img-fluid" src="~/images/ProductImg/{{q.ProductImage}}" loading="lazy" alt="product-hover-image" loading="lazy" alt="">
                                                                            </a>
                                                                        </div>
                                                                        <div class="product-badges product-badges-position product-badges-mrg" ng-show="q.PLabel!=''">
                                                                            <span class="hot">{{q.PLabel}}</span>
                                                                        </div>
                                                                    </div>
                                                                    <div class="product-content-wrap">
                                                                        <h2><a href="/Home/Gift?url={{q.PUrl}}">{{q.ProductTitle}}</a></h2>
                                                                        <div class="product-price">
                                                                            <span><i class="fa fa-inr" aria-hidden="true"></i> {{q.Price}}/-</span>
                                                                        </div>
                                                                        <div class="d-flex justify-content-between">
                                                                            <div class="button-design mt-2">
                                                                                <a aria-label="Add To Cart" data-bs-toggle="offcanvas" data-bs-target="#offcanvasRight" aria-controls="offcanvasRight" class="btn theme-button-2" ng-click="addItemToCart($index)"><i class="fi-rs-shopping-bag-add"></i> Add To Cart </a>
                                                                            </div>
                                                                            @* Popup Button For Image *@
                                                                            <div class="button-design mt-2" ng-if="!q.Video">
                                                                                <a aria-label="View Image" class="btn theme-button-3 d-flex gap-2 align-items-center" ng-click="quickview(q)"><span class="d-lg-none d-sm-none d-block fs-9">View </span><i class="fa fa-eye" aria-hidden="true"></i></a>
                                                                            </div>


                                                                            @* Popup Button For Video *@
                                                                            <div class="button-design mt-2" ng-if="q.Video">
                                                                                <a aria-label="View Image" class="btn theme-button-3 d-flex gap-2 align-items-center for-video" ng-click="quickview(q)"><span class="d-lg-none d-sm-none d-block fs-9 hide-animation" id="hideText">View </span><i class="fa fa-youtube-play" aria-hidden="true"></i></a>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </section>


                    <section class="product-tabs section-padding mb-0 pb-1" ng-show="ProductData.length>0">
